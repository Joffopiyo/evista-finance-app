# Frontend Environment Variables for Vercel Deployment
# Copy this file to .env.local for local development
# On Vercel, add these as Environment Variables in project settings

# API Backend URL (Render service)
VITE_API_URL=https://evista-finance-app-backend.onrender.com/api

# Frontend URL (Vercel domain)
VITE_FRONTEND_URL=https://evista-finance-app.vercel.app

# Node environment
VITE_NODE_ENV=production
